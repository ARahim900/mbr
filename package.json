{
  "name": "muscat-bay-assets-and-operation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "build:vercel": "npm ci --legacy-peer-deps && NODE_ENV=production vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "verify": "node scripts/verify-commit.js",
    "setup-hooks": "bash scripts/setup-hooks.sh",
    "setup-hooks:windows": "node scripts/setup-hooks-windows.js",
    "rollback": "node scripts/rollback-helper.js",
    "commit-helper": "git add . && git commit",
    "lint:check": "node scripts/verify-commit.js",
    "postinstall": "npm run setup-hooks:windows 2>nul || npm run setup-hooks 2>/dev/null || echo 'Hooks setup skipped'",
    "clean": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json && if exist dist rmdir /s /q dist",
    "clean-install": "npm run clean && npm install",
    "vercel-build": "node scripts/vercel-build.js",
    "build:production": "node scripts/build-production.js",
    "fix-lockfile": "node scripts/fix-lockfile.js",
    "validate-lockfile": "node scripts/validate-lockfile.js",
    "build:vercel-safe": "node vercel-build-safe.js",
    "fix-console-logs": "node scripts/remove-console-logs.js",
    "deploy:quick": "npm run fix-console-logs && git add . && git commit --no-verify -m 'fix: remove console logs for deployment' && git push origin main"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@supabase/supabase-js": "^2.50.4",
    "@tanstack/react-query": "^5.83.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "aos": "^3.0.0-beta.6",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.417.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.8.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.0.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/rollup-win32-x64-msvc": "^4.45.1",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/aos": "^3.0.7",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "depcheck": "^1.4.7",
    "esbuild": "0.25.6",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup": "^4.21.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "ts-prune": "^0.10.3",
    "typescript": "~5.7.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.8",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "4.21.2",
    "@rollup/rollup-linux-x64-gnu": "4.21.2",
    "@rollup/rollup-win32-x64-msvc": "4.21.2"
  },
  "overrides": {
    "@rollup/rollup-win32-x64-msvc": "4.21.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
