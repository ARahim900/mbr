{
  "name": "muscat-bay-assets-and-operation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "npm@10.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ARahim900/mbr-magic-box.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run lint && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"components/**/*.{ts,tsx}\" \"hooks/**/*.{ts,tsx}\" \"utils/**/*.{ts,tsx}\" \"database/**/*.{ts,tsx}\" \"services/**/*.{ts,tsx}\" \"store/**/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\" \"types.ts\" \"App.tsx\" \"index.tsx\" --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,html}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md,html}\"",
    "verify": "npm run type-check && npm run lint && npm run test:run",
    "prepare": "husky || true",
    "pre-commit": "lint-staged",
    "setup-hooks": "bash scripts/setup-hooks.sh",
    "setup-hooks:windows": "node scripts/setup-hooks-windows.js",
    "rollback": "node scripts/rollback-helper.js",
    "commit-helper": "git add . && git commit",
    "postinstall": "npm run prepare 2>/dev/null || echo 'Husky setup skipped'",
    "clean": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json && if exist dist rmdir /s /q dist",
    "clean-install": "npm run clean && npm install",
    "fix-console-logs": "node scripts/remove-console-logs.js",
    "fix-design": "node scripts/fix-design-violations.js",
    "deploy:quick": "npm run fix-console-logs && git add . && git commit --no-verify -m 'fix: remove console logs for deployment' && git push origin main",
    "deploy:clean": "npm run fix-console-logs && npm run fix-design && git add . && git commit --no-verify -m 'fix: clean up code for deployment' && git push origin main"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
